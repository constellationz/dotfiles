#!/usr/bin/env lua
-- Make things fancy

local letters = { a = "ğ•’", b = "ğ•“", c = "ğ•”", d = "ğ••", e = "ğ•–", f = "ğ•—", g = "ğ•˜", h = "ğ•™", i = "ğ•š", j = "ğ•›", k = "ğ•œ", l = "ğ•", m = "ğ•", n = "ğ•Ÿ", o = "ğ• ", p = "ğ•¡", q = "ğ•¢", r = "ğ•£", s = "ğ•¤", t = "ğ•¥", u = "ğ•¦", v = "ğ•§", w = "ğ•¨", x = "ğ•©", y = "ğ•ª", z = "ğ•«", A = "ğ”¸", B = "ğ”¹", C = "â„‚", D = "ğ”»", E = "ğ”¼", F = "ğ”½", G = "ğ”¾", H = "â„", I = "ğ•€", J = "ğ•", K = "ğ•‚", L = "ğ•ƒ", M = "ğ•„", N = "â„•", O = "ğ•†", P = "â„™", Q = "â„š", R = "â„", S = "ğ•Š", T = "ğ•‹", U = "ğ•Œ", V = "ğ•", W = "ğ•", X = "ğ•", Y = "ğ•", Z = "â„¤" }

local input = io.read("*a")
local out = ""
for i = 1, #input do
	local c = input:sub(i, i)
	local o = letters[c] or c
	out = out .. o
end
print(out)
